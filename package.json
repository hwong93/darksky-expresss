{
  "name": "toast-test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist && cp ./src/views/* ./dist/views",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.17.3",
    "express-validator": "^6.1.1",
    "geoip-lite": "^1.3.7",
    "gulp-csso": "^3.0.1",
    "gulp-twig": "^1.2.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "twig": "~0.10.3",
    "isomorphic-fetch": "^2.2.1",
    "dotenv": "^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "gulp": "^4.0.2",
    "gulp-sass": "^4.0.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1"
  }
}
